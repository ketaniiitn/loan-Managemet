{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
   
    "esModuleInterop": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"]
    },
    "moduleResolution": "bundler",
"allowImportingTsExtensions": true,
"noEmit": true

  },
  "include": ["src"]
}
